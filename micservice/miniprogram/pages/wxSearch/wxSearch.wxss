.search-section{
  height: 100%;
  border-top: 1px #eee solid;
  background-color: rgba(0, 0, 0, 0.2);
  margin: 20rpx;

}
.search-section .search-pancel{
  display: flex;
  flex-direction: row;
  align-items: center;

}
.search-section .search-pancel input{
  flex: 8; 
  background-color: #eee;
  padding: 5px 0 5px 10px;
  font-size: 30rpx;
}
.search-section .search-pancel button{
  border: none !important;
  color: cornflowerblue !important;

}


.userEval{
  margin: 20rpx 20rpx;
  line-height: 60rpx;

}
.userEval .userEvalutes{
  margin: 30rpx 0;
}

.userEval .userEvalutes .userEvalutes-tasks-evalute{
  display: flex;
  flex-direction: column;
}
.userEval .userEvalutes .userEvalutes-tasks-evalute .star{
  display: flex;
  flex-direction: row;
  align-items: center;

}
.userEval .userEvalutes .userEvalutes-tasks-evalute .star image{
  width: 224rpx;
  height: 33rpx;
}

.userEval .userEvalutes .userEvalutes-tasks-evalute .hotevalute{
  display: flex;
  flex-direction: row;
}
.userEval .userEvalutes .userEvalutes-tasks-evalute .hotevalute .hotevalute-item{

  background-color: rgb(219, 214, 214);
  margin-left: 20rpx;
  padding: 0 10rpx;
  border-radius: 18rpx;
  font-size: 30rpx;

}

.myuserInfo{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.myuserInfo .myuserInfo-userInfo{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.myuserInfo text{
  font-size: 25rpx;
  color: rgb(153, 151, 151);
}
.myuserInfo image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 20%;
}
.indexshow{
  font-size: 35rpx;
  margin: 40rpx 40rpx;
  color: rgb(107, 106, 106);
}
.loading{
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 21px 0;
  font-size: 35rpx;

}