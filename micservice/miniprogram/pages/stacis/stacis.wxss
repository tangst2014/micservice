
.opendata{
  height: 328rpx;
  margin-bottom: 20rpx;

}


.selected{
  padding: 2rpx 30rpx 2rpx 30rpx;
  background-color: rgb(233, 154, 6);
  color: white;
}

.unselected{
  padding: 2rpx 30rpx 2rpx 30rpx;
  background-color: #f6f6f6;
  color: black;
}
.scroll-view_H{
  white-space: nowrap;
}
.scroll-view-item{
  height: 100rpx
}
.scroll-view-item_H{
  display: inline-block;
  width: 183rpx;
  font-size: 30rpx;
  border: 1px solid rgb(129, 126, 126);
  margin: 0 20rpx;
}
.scroll-view-item-user{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 10rpx 0;

}


.taskmarksclass{
  margin: 20rpx;
  display: flex;
  flex-wrap: wrap;

}
.taskmarksclass .taskmarks-item{
  display: flex;
  margin: 5px;
  background-color:rgb(245, 244, 223);
  padding: 10rpx 24rpx;
  font-size: 28rpx;
  border-radius: 15rpx;

}


.userEval{
  margin: 20rpx 20rpx;
  line-height: 60rpx;

}
.userEval .userEvalutes{
  margin: 30rpx 0;
}

.userEval .userEvalutes .userEvalutes-tasks-evalute{
  display: flex;
  flex-direction: column;
}
.userEval .userEvalutes .userEvalutes-tasks-evalute .star{
  display: flex;
  flex-direction: row;
  align-items: center;

}
.userEval .userEvalutes .userEvalutes-tasks-evalute .star image{
  width: 224rpx;
  height: 33rpx;
}

.userEval .userEvalutes .userEvalutes-tasks-evalute .hotevalute{
  display: flex;
  flex-direction: row;
}
.userEval .userEvalutes .userEvalutes-tasks-evalute .hotevalute .hotevalute-item{

  background-color: rgb(219, 214, 214);
  margin-left: 20rpx;
  padding: 0 10rpx;
  border-radius: 18rpx;
  font-size: 30rpx;

}

.userEval .userEvalutes .userEvalutes-department{
  font-weight: bold;
  display: flex;
  justify-content: space-between;

}

.userEval .userEvalutes .userEvalutes-time{
  
}

.userEval .userEvalutes .userEvalutes-tasks{
 
}

.userEval .userEvalutes .userEvalutes-doing{
   
}

.myuserInfo{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.myuserInfo .myuserInfo-userInfo{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.myuserInfo text{
  font-size: 25rpx;
  color: rgb(153, 151, 151);
}
.myuserInfo image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 20%;
}
